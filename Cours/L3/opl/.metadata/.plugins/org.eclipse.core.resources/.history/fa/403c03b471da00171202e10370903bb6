int x1=...;
int x2=...;
int x3=...;
int x4=...;
minimize
  	sum(i in 1..4)(5*x1+4*x2+3*x3+2*x4) ;
  	subject to{
  				6*x1+4*x2+5*x3+1*x4 <= 10;
  				//2*x1+3*x2+3*x3+3*x4 <= 10; contrainte relachée !
  				//1*x1+5*x2+5*x3+1*x4 <= 01; contrainte relachée !
  	  	
  	}